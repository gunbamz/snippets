{
  "manifest_version": 3,
  "name": "REACT-APP",
  "version": "1.0",
  "options_page": "index.html",
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon-34.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": [
    {"128": "icon-128.png"}
  ],
  "permissions": [
    "activeTab",
    "tabs",
   "notifications",
    "storage",
    "scripting"
  ],
  "host_permissions": [
      "https://www.binance.com/*"
  ]
}
